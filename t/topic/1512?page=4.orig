n=0
si=0
sil=0
t0=time.time()
for a in tp:
    n2=0
    if n%100000==0:print('a: %d'%n)
    t2=time.time()
    for b in op:
        if n2%100000==0:
            print('b: %d'%n2,si);sil=0
            t3=time.time()
            print('= time: %f'%(t3-t2))
            t2=time.time()
        a2=eval(a)
        b2=eval(b)
        ve=[]
        for c in a2.keys():
            if c not in ve:ve.append(c)
        for c in b2.keys():
            if c not in ve:ve.append(c)
        vea=[]
        veb=[]
        for c in ve:
            if c in a2:
                vea.append(a2[c])
            else:
                vea.append(0)
            if c in b2:
                veb.append(b2[c])
            else:
                veb.append(0)
        si=ccos(vea,veb)
        if si&gt;sil:sil=si;print(sil)
        if si&gt;0.4:
            a3=eval(tt[n])
            b3=eval(ot[n2])
            if not os.path.exists(pa:='target/%s'%a3[1]):os.makedirs(pa)
            f=open(dn:='%s/%s.txt'%(pa,str(n2).rjust(10).replace(' ','0')),'w+');f.write('\n'.join([tt[n],ot[n2],str(si)]));f.close()
            print(dn)
        n2+=1
    t1=time.time()
    print('time: %f'%(t1-t0))
    t0=time.time()
    n+=1
