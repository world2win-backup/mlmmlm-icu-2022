z=0
zl=[]
afi=[]
if os.path.exists('afi_txt.list'):
    try:
        f=open('afi_txt.list','r');t=f.read();f.close()
        afi=eval(t)
    except:pass
fl=[]
for a,b,c in os.walk(sys.path[0]):
    for d in c:
        if d[-3:]=='pdf':fl.append([a,d])
zl=len(fl)
for h in fl:
    a,d=h[0],h[1]
    z+=1
    af='%s/%s.txt'%(a,d[:-4])
    if not af in afi:
        t=''
        for m,n,o in os.walk(p:='%s/%s ppOCR'%(a,d[:-4])):
            n=0
            o=[[v,int(v[:-5])]for v in o]
            o.sort(key=lambda x:x[1])
            o=[v[0]for v in o]
            for i in o:
                n+=1
                if n!=len(o)-1 and n!=1:t='%s\n\n'%(t)
                im='%s/%s'%(m,i)
                f=open(im,'r');l=eval(f.read());f.close()
                for z in range(len(l)-1):
                    if z%2==0:
                        if z+1&lt;=len(l)-1:
                            if(dd:=abs(l[z][0][0][1]-l[z+1][0][0][1]))&lt;8:
                                t='%s%s%s'%(t,l[z][1][0],l[z+1][1][0])
                            else:
                                t='%s%s\n%s'%(t,l[z][1][0],l[z+1][1][0])
                        else:
                            if(dd:=abs(l[z-1][0][0][1]-l[z][0][0][1]))&lt;8:
                                t='%s%s'%(t,l[z][1][0])
                            else:
                                t='%s\n%s'%(t,l[z][1][0])
        f=open(af,'w+');f.write(t);f.close()
        afi.append(af);f=open('afi_txt.list','w+');f.write(repr(afi));f.close()
        restart()
