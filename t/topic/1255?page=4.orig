        f=open('afi_ocr.list','r');t=f.read();f.close()
        afi=eval(t)
    except:pass
fl=[]
for a,b,c in os.walk('/home/a/METADATA'):
    for d in c:
        if d[-3:]=='pdf':fl.append([a,d])
zl=len(fl)
for h in fl:
    a,d=h[0],h[1]
    z+=1
    n='%s/%s ppOCR'%(a,d[:-4])
    if not os.path.exists(n):os.mkdir(n)
    af='%s/%s ppOCR'%(a,d[:-4])
    if not af in afi:
        for m,n,o in os.walk(p:='%s/%s'%(a,d[:-4])):
            for i in o:
                dn='%s/%s.list'%(af,i[:-4])
                if not os.path.exists(dn):
                    print(im:='%s/%s'%(m,i),'===== Start OCR.')
                    re=ocr.ocr(im,cls=True)
                    f=open(dn,'w+');f.write(repr(re));f.close()
                    print(re)
                    print(im,'===== OCR end.')
        afi.append(af);f=open('afi_ocr.list','w+');f.write(repr(afi));f.close()
        restart()
